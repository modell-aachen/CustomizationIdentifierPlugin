{
    "outputsuffix": "_Customer_Overview_Customizing",
    "columntitles": "File,Type,Path,URL",
    "rules": [
        {
            "type": "file",
            "outputtype": "Skin-Template",
            "path": "data/Custom",
            "name": "*Template",
            "filetype": ["txt"],
            "ignore": {
                "subpath": ["*,pfv"],
                "name": [
                    "WebAtom",
                    "WebChanges",
                    "WebContentTemplates",
                    "WebCreateNewTopic",
                    "WebHome",
                    "WebIndex",
                    "WebNotify",
                    "WebPreferences",
                    "WebRss",
                    "WebSearch",
                    "WebSearchAdvanced",
                    "WebStatistics",
                    "WebTopicList"
                ],
                "filetype": []
            }
        },
        {
            "type": "file",
            "outputtype": "Custom-File",
            "path": "data/Custom",
            "name": "*",
            "filetype": ["*"],
            "ignore": {
                "subpath": ["*,pfv"],
                "name": [
                    "WebAtom",
                    "WebChanges",
                    "WebContentTemplates",
                    "WebCreateNewTopic",
                    "WebHome",
                    "WebIndex",
                    "WebNotify",
                    "WebPreferences",
                    "WebRss",
                    "WebSearch",
                    "WebSearchAdvanced",
                    "WebStatistics",
                    "WebTopicList",
                    "*Template"
                ],
                "filetype": ["*"]
            }
        },
        {
            "type": "file",
            "outputtype": "Custom-Pub-File",
            "path": "pub/Custom",
            "name": "*",
            "filetype": [],
            "ignore": {
                "subpath": ["*,pfv"],
                "name": [],
                "filetype": []
            }
        }, 
        {
            "type": "file",
            "outputtype": "System-Template",
            "path": "templates",
            "name": "*.custom.*",
            "filetype": ["*"],
            "ignore": {
                "subpath": [],
                "name": [],
                "filetype": []
            }
        },
        {
            "type": "plugin",
            "standard": [
                "ActionTrackerPlugin",
                "AmpelPlugin",
                "AppManagerPlugin",
                "AttachmentFilterPlugin",
                "AttachmentListPlugin",
                "AutoTemplatePlugin",
                "BreadCrumbsPlugin",
                "BuildContrib",
                "CKEditorPlugin",
                "CustomizationIdentifierPlugin",
                "CustomMaketextPlugin",
                "DBCachePlugin",
                "DefaultPreferencesPlugin",
                "DiagnoseLinkPlugin",
                "DnDUploadPlugin",
                "EmployeesAppPlugin",
                "ExportExcelPlugin",
                "FavoritesPlugin",
                "FilesysVirtualPlugin",
                "FillWebsPlugin",
                "FilterPlugin",
                "FlatSkinPlugin",
                "FlexFormPlugin",
                "FlexWebListPlugin",
                "FlowchartEditorPlugin",
                "FormGeneratorPlugin",
                "GenPDFPrincePlugin",
                "GlossarPlugin",
                "ImagePlugin",
                "JQGridPlugin",
                "JQPluginContribPlugin",
                "JEditableContribPlugin",
                "JQTableSorterPlugin",
                "JQueryPlugin",
                "JSi18nPlugin",
                "KVPPlugin",
                "LdapNgPlugin",
                "LinkSymbolsPlugin",
                "MailerContribPlugin",
                "MaintenancePlugin",
                "MAPrinceModPlugin",
                "MetaCommentPlugin",
                "MetaDataPlugin",
                "MimeIconPlugin",
                "ModacContextMenuPlugin",
                "ModacSkin",
                "MoreFormfieldsPlugin",
                "NewUserPlugin",
                "NominalPlugin",
                "NotifyAffectedPlugin",
                "NotifyMetaChangedPlugin",
                "NumberTopicsPlugin",
                "ProjectsProgressPlugin",
                "QueryUserPlugin",
                "QueryVersionPlugin",
                "RedirectPlugin",
                "RenderPlugin",
                "RenderListPlugin",
                "RestoreAttachmentPlugin",
                "RootJobPlugin",
                "SearchGridPlugin",
                "SetVariablePlugin",
                "SolrPlugin",
                "SortSolrByChapterPlugin",
                "SwitchMacroPlugin",
                "TaskDaemonPlugin",
                "TasksAPIPlugin",
                "TopicDataHelperPlugin",
                "TwistyPlugin",
                "UnifiedAuthPlugin",
                "VueJSPlugin",
                "WebAutoIncPlugin",
                "WebLinkPlugin",
                "WebPrefPlugin",
                "WysiwygPlugin"
            ],
            "outputtype": "Plugin"
        },
        {
            "type": "contrib",
            "standard": [
                "CompareRevisionsAddOn",
                "DBCacheContrib",
                "FontAwesomeContrib",
                "JQMomentContrib",
                "JQPluginContrib",
                "JQPrettyPhotoContrib",
                "JQSelect2Contrib",
                "JQSerialPagerContrib",
                "JQTwistyContrib",
                "JSCalendarContrib",
                "LdapContrib",
                "MailTemplatesContrib",
                "ManualsAppContrib",
                "MinutesAppContrib",
                "ModacSkin",
                "PickADateContrib",
                "PlainFileStoreContrib",
                "ProcessesContentContrib",
                "QwikiContrib",
                "QwikiFormfieldsContrib",
                "ResubmissionContrib",
                "RisksAppContrib",
                "RollenAppContrib",
                "StandardsAppContrib",
                "StringifierContrib",
                "SwitchableLoginManagerContrib"
            ],
            "outputtype": "Contrib"
        },
        {
            "type": "sitepref",
            "outputtype": "CSS",
            "preference": "FOSWIKI_PROJECT_URL",
            "standardvalue": "",
            "standardtype": "path"
        },
        {
            "type": "sitepref",
            "outputtype": "Skin-Favicon",
            "preference": "FAVICON",
            "standardvalue": "/pub/System/ModacSkinTheme/ModacSkinLogos/favicon.ico",
            "standardtype": "path"
        },
        {
            "type": "sitepref",
            "outputtype": "Skin-Logo",
            "preference": "WIKILOGOIMG",
            "standardvalue": "/pub/System/ModacSkinTheme/ModacSkinLogos/qwiki-logo_small.png",
            "standardtype": "path"
        },
        {
            "type": "sitepref",
            "preference": "WIKITOOLNAME",
            "standardvalue": "Q.wiki"
        },
        {
            "type": "webpref",
            "action": "list",
            "webs": [
            ],
            "ignore": {
                "name": [
                    "Trash"
                ]
            },
            "preferences": [
                "DENYWEBVIEW",
                "ALLOWWEBVIEW",
                "DENYWEBCHANGE",
                "ALLOWWEBCHANGE",
                "DENYWEBRENAME",
                "ALLOWWEBRENAME",
                "ALLOWTOPICCHANGE",
                "ALLOWTOPICRENAME"
            ]
        }
    ]
}
